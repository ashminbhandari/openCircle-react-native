{
  "expo": {
    "name": "openCircle",
    "slug": "openCircle",
    "scheme": "open-circle",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/opencircle-icon.png",
    "splash": {
      "image": "./assets/opencircle-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cp.opencircle.bi",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "openCircle uses your location to display you on the world map."
      }
    }
  }
}
