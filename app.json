{
  "expo": {
    "name": "openCircle for Spotify - Word Server",
    "slug": "open-circle-for-spotify-world-server",
    "scheme": "open-circle",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/opencircle-logo.png",
    "splash": {
      "image": "./assets/opencircle-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cp.opencircle.bi",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "openCircle uses a user's location to display them on a map"
      }
    }
  }
}
